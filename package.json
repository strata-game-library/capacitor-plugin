{
  "name": "@jbcom/strata-capacitor-plugin",
  "version": "0.0.1",
  "description": "Capacitor plugin for Strata 3D - cross-platform input, device detection, and haptics for mobile games",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/"
  },
  "keywords": ["capacitor", "strata", "mobile", "input", "haptics"],
  "author": "Jon Bogaty <jon@jonbogaty.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbcom/nodejs-strata-capacitor-plugin"
  },
  "peerDependencies": {
    "@jbcom/strata": ">=1.4.0",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@capacitor/core": "^8.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@capacitor/cli": "^8.0.0",
    "@types/react": "^19.0.0",
    "rimraf": "^6.1.0",
    "typescript": "^5.9.0"
  }
}
